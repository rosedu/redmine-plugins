#Routes for the Upload Forms plugin
ActionController::Routing::Routes.draw do |map|
  map.resources :projects do |project|
   project.resources :uploads, :shallow => true  do |upload|
     #Using REST-ful routes
#      upload.uploadFile 'uploadFile' , :action => 'addFiles'
     end   
   end

   map.with_options :controller => 'uploads' do |upl|
     upl.addFiles ' ', :action => 'addFiles'
   end
end

