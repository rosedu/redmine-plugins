<h2>Upload a new file</h2>
<% form_for :upload, :multipart => true  do  |f| %>
  <p>
    <label_for = "upload_file_path">File Path:</label>
    <%= file_field_tag :up_file %>
  </p>
  <%= f.submit "Create" %>
<% end %>

<p>
  <% debug @project%>
  <%= link_to 'Back',  {:controller => 'uploads', :action => 'index'} -%>
</p>
